{
    "comments": {
        "lineComment": "'"
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""]
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""]
    ],
    "indentationRules": {
        "increaseIndentPattern": "^\\s*(If|For|While|Sub|Function|Class|Module|Try|Catch|Type|Select|With|Do)\\b.*$",
        "decreaseIndentPattern": "^\\s*(End\\s+(If|For|While|Sub|Function|Class|Module|Try|Type|Select|With)|Next|Catch|Finally|Loop|Case)\\b.*$"
    },
    "folding": {
        "markers": {
            "start": "^\\s*(Sub|Function|Class|Module|If|For|While|Try|Type|Select|With|Do)\\b",
            "end": "^\\s*(End\\s+(Sub|Function|Class|Module|If|For|While|Try|Type|Select|With)|Next|Catch|Finally|Loop|Case)\\b"
        }
    },
    "wordPattern": "[A-Za-z_][A-Za-z0-9_]*"
}